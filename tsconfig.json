{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "baseUrl": "./src",
    "paths": {
      "@env": [
        "environments/environment"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@models/*": [
        "app/models/*"
      ],
      "@services/*": [
        "app/services/*"
      ],
      "@pages/*": [
        "app/pages/*"
      ],
      "@pipes/*": [
        "app/pipes/*"
      ]
    }
  }
}
